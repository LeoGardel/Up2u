#dashboard_container div{
padding: 0;
}

#dashboard_container {
position: absolute;
top: 0;
left: 0;
margin-top: 102px;
height: -moz-calc(100% - 102px);
height: -webkit-calc(100% - 102px);
height: -o-calc(100% - 102px);
height: calc(100% - 102px);
width: 100%;
overflow: hidden;

font-size: 0;
padding: 0;
}

.largura_figura {
width: -moz-calc(100% - 30px);
width: -webkit-calc(100% - 30px);
width: -o-calc(100% - 30px);
width: calc(100% - 30px);
}

#figura_niveis {
position: relative;

height: 100%;
background-image: url('<%= image_path 'dashboard/niveis.jpg' %>');
background-size: 100% 100%;
}

.tracejado_vert {
width: 2px;
height: 100%;
background-image: url('<%= image_path 'dashboard/tracejado_vert.jpg' %>');
background-repeat: repeat-y;
display: table-cell;
}

.celula_bandeira_rodape {
display: table-cell;
position: relative;
}

.bandeira_rodape {
width: 52%;
margin-top: 3px;
margin-bottom: 15px;
}

.container_lista {
width: 100%;
height: 100%;
position: relative;
}

.container_absoluto_lista {
position: absolute;
width:100%;
height: 100%;
top: 0;
left: 0;
}

.celula_competencia {
width: 100%;
margin-top: 15px;
position: relative;
}

.texto_principal_competencia {
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
font-size: 10pt;
color: #686868;
cursor: pointer;
}

.texto_secundario_competencia {
font-family: "nexa-light", Arial, Helvetica, sans-serif;
font-size: 9pt;
text-decoration: none;
}

.tooltip_competencia {
z-index: 10;
display: none;
margin-top: 20px;
left: -moz-calc((100% - 150px)/2);
left: -webkit-calc((100% - 150px)/2);
left: -o-calc((100% - 150px)/2);
left: calc((100% - 150px)/2);
width: 150px;
height: 110px;
text-align: left;
-webkit-box-shadow:  0px 0px 8px 1px rgba(200, 200, 200, 1);
box-shadow:  0px 0px 8px 1px rgba(200, 200, 200, 1);
position: relative;
}

span.texto_principal_competencia:hover div {
display: inline;
position: absolute;
background-color: #f6f6f6;
}

.texto_tooltip {
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
font-size: 9pt;
color: #686868;
padding-top: 10px;
position: absolute;
left: 15%;
}

.seta_tooltip {
width: 70%;
left: 15%;
position: absolute;
height: 24%;
background-size: 100% 100%;
}

.ret_colorido_seta_tooltip {
height: 39%;
top: 31%;
left: 1px;
text-align: center;
font-size: 7pt;
}

.palavras_niveis {
position: absolute;
left: 0;
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
font-size: 13pt;
color: #4f5557;
}

.textos_esq {
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
font-size: 16pt;
color: #4f5557;
}

.textos_dir {
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
font-size: 20pt;
color: #4f5557;
}

.container_texto_abas {
width: 100%;
height: 100%;
display: table;
text-align: center;
}

#texto_aba_esq {
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
color: #33b8c0;
display: table-cell;
vertical-align: middle;
}

#texto_aba_dir {
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
font-size: 16pt;
color: #44494b;
display: table-cell;
vertical-align: middle;
}

#retangulo_verde_seta {
position: absolute;
width: 3.6%;
right: 2.4%;
bottom: 1px;

background: #00e600; /* Old browsers */
background: -moz-linear-gradient(top,  #00e600 0%, #00c100 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#00e600), color-stop(100%,#00c100)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #00e600 0%,#00c100 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #00e600 0%,#00c100 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #00e600 0%,#00c100 100%); /* IE10+ */
background: linear-gradient(to bottom,  #00e600 0%,#00c100 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00e600', endColorstr='#00c100',GradientType=0 ); /* IE6-9 */
}

#retangulo_suporte_porcent {
position: absolute;
height: 35px;
width: -moz-calc(5.9% + 30px);
width: -webkit-calc(5.9% + 30px);
width: -o-calc(5.9% + 30px);
width: calc(5.9% + 30px);
left: 94%;
}

#linha_suporte_porcent {
position: absolute;
bottom: 0;
opacity: 0.31;
filter: alpha(opacity=31);
border: 0;
height: 1px;
}

#retangulo_porcent {
position: absolute;
top: 0;
right: 0;
}

#retangulo_porcent_voce {
position: absolute;
bottom: 0;
right: 0;
}

#porcent_total {
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
font-size: 13pt;
color: #4f5557;
}

#voce_figura {
font-family: "nexa-light", Arial, Helvetica, sans-serif;
font-size: 9pt;
color: #4f5557;
}

.texto_bandeira_sup {
font-family: "nexa-bold", Arial, Helvetica, sans-serif;
font-size: 15pt;
color: #595959;
}

.texto_bandeira_inf {
font-family: "nexa-light", Arial, Helvetica, sans-serif;
font-size: 11pt;
color: #595959;
}

.cont_bandeira_sup {
position: absolute;
width: 100%;
left: 0;
top: 12%;
text-align: center;
}

.cont_bandeira_inf {
position: absolute;
top: -moz-calc(12% + 21px);
top: -webkit-calc(12% + 21px);
top: -o-calc(12% + 21px);
top: calc(12% + 21px);
width: 100%;
left: 0;
text-align: center;
}

#voce_boneco {
font-family: "nexa-light", Arial, Helvetica, sans-serif;
font-size: 9pt;
color: #ffffff;
}

#container_voce {
background-color: #40b637;
border: 1px solid #606060;
}

#container_voce_boneco {
width: 35px;
position: absolute;

right: -moz-calc(12.5% - 17.5px);
right: -webkit-calc(12.5% - 17.5px);
right: -o-calc(12.5% - 17.5px);
right: calc(12.5% - 17.5px);
top: 17%;
}